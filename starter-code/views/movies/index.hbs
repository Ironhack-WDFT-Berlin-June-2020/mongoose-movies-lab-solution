<h1>Movies</h1>
{{#each movies}}
    <h2>{{this.title}}</h2>
    {{#each this.cast}}
        <li><strong>Name: </strong>{{ name }} || <strong>Catchphrase:</strong> {{ catchPhrase }}</li>
    {{/each}}
    <a href="/movies/{{ this._id }}/edit">Edit</a>
{{/each}}
